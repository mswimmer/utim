    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#hasDetection -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#hasDetection">
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Detection"/>
        <rdfs:comment xml:lang="en">What form of detection can detection this attack pattern.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#hasObjective -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#hasObjective">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Campaign"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/member"/>
                        <owl:someValuesFrom rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Objective"/>
        <rdfs:comment xml:lang="en">State what objectives a Threat Actor or Campaign may have</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#hasTechnique -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#hasTechnique">
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Technique"/>
    </owl:ObjectProperty>
    


