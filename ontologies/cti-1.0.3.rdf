        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#DetectionMethod"/>
        <rdfs:comment xml:lang="en">A defensive measure or procedure that will be bypassed a given attack pattern.</rdfs:comment>
    </owl:ObjectProperty>
    


