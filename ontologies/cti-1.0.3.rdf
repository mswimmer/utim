                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty <http://xmlns.com/foaf/0.1/member> ;
                                                owl:someValuesFrom <http://xmlns.com/foaf/0.1/Person>
                                              ]
                                            )
                              ] ;
          <http://ontologydesignpatterns.org/opla-sd#entityPosition> [ <http://ontologydesignpatterns.org/opla-sd#entityPositionX> "223.6316155482985"^^xsd:double ;
