        <rdf:type rdf:resource="http://ontologies.ti-semantics.com/cti#MalwareLabel"/>
        <rdfs:comment xml:lang="en">A malicious computer program that replicates by reproducing itself or infecting other programs by modifying them.</rdfs:comment>
        <rdfs:label xml:lang="en">virus</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://ontologies.ti-semantics.com/cti#vulnerability-report -->

