    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/core#AdversarialActivity -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#AdversarialActivity"/>
    


    <!-- http://ontologies.ti-semantics.com/core#Attack -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Attack"/>
    


    <!-- http://ontologies.ti-semantics.com/core#Catalog -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Catalog"/>
    


    <!-- http://ontologies.ti-semantics.com/core#CourseOfAction -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#CourseOfAction">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#mitigates"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                            <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Tool"/>
                            <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Vulnerability"/>
                            <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/core#Malware -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Malware">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#uses"/>
                                <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/platform#platform"/>
                                <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/core#Platform"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#revoked"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#MaliciousBehavior"/>
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#Observable"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Software"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#implements"/>
                        <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/cti#Technique"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">TODO: Need to differentiate between Tool and Malware so that we can create a necessary and sufficent equivalent class declaration</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/core#Platform -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Platform"/>
    


    <!-- http://ontologies.ti-semantics.com/core#Software -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Software"/>
    


    <!-- http://ontologies.ti-semantics.com/core#ThreatActor -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#ThreatActor">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Organization"/>
                            <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Person"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/member"/>
                                <owl:someValuesFrom rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#hasObjective"/>
                        <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/cti#MaliciousObjective"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#Observable"/>
        <rdfs:comment xml:lang="en">It is nessary and sufficient that a ThreatActor is either a person, an organization or a group of people and that they have malicious objectives.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/core#Tool -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Tool">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/platform#platform"/>
                <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/core#Platform"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#Behavior"/>
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#Observable"/>
        <rdfs:comment xml:lang="en">TODO: Need to differentiate between Tool and Malware so that we can create a necessary and sufficent equivalent class declaration</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/core#Vulnerability -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/core#Vulnerability"/>
    


    <!-- http://ontologies.ti-semantics.com/cti#AerospaceIndustrySector -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/cti#AerospaceIndustrySector">
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#IndustrySector"/>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/cti#AgricultureIndustrySector -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/cti#AgricultureIndustrySector">
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#IndustrySector"/>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/cti#AnomalousActivity -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/cti#AnomalousActivity">
        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/core#Activity"/>
        <rdfs:comment xml:lang="en">Unexpected, or unusual activity that may not necessarily be malicious or indicate compromise. This type of activity may include reconnaissance-like behavior such as port scans or version identification, network behavior anomalies, and asset and/or user behavioral anomalies.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ontologies.ti-semantics.com/cti#AnomalousActivityIndicator -->

    <owl:Class rdf:about="http://ontologies.ti-semantics.com/cti#AnomalousActivityIndicator">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Indicator"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://ontologies.ti-semantics.com/cti#detects"/>
                        <owl:someValuesFrom rdf:resource="http://ontologies.ti-semantics.com/cti#AnomalousActivity"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">An Indicator of unexpected, or unusual activity that may not necessarily be malicious or indicate compromise. This type of activity may include reconnaissance-like behavior such as port scans or version identification, network behavior anomalies, and asset and/or user behavioral anomalies.</rdfs:comment>
    </owl:Class>
    


