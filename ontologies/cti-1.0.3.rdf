        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/core#Malware"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/core#Malware"/>
    </owl:ObjectProperty>
    


