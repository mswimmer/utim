    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#impersonates">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/core#ThreatActor"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:label xml:lang="en">impersonates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#implements -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#implements"/>
    


    <!-- http://ontologies.ti-semantics.com/cti#indicates -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#indicates">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#Indicator"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#ThreatActor"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Tool"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Campaign"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This Relationship describes that the Indicator can detect evidence of the related Campaign, Intrusion Set, or Threat Actor. This evidence may not be direct: for example, the Indicator may detect secondary evidence of the Campaign, such as malware or behavior commonly used by that Campaign.

For example, an indicates Relationship from an Indicator to a Campaign object representing Glass Gazelle means that the Indicator is capable of detecting evidence of Glass Gazelle, such as command and control IPs commonly used by that Campaign.</rdfs:comment>
        <rdfs:label xml:lang="en">indicates</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#intrusionSetAttributedTo -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#intrusionSetAttributedTo">
        <rdfs:subPropertyOf rdf:resource="http://ontologies.ti-semantics.com/cti#attributedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/core#ThreatActor"/>
        <rdfs:comment xml:lang="en">This Relationship describes that the related Threat Actor is involved in carrying out the Intrusion Set.

For example, an attributed-to Relationship from the Red Orca Intrusion Set to the Urban Fowl Threat Actor means that the actor carried out or was involved in some of the activity described by the Intrusion Set.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#killChainPhase -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#killChainPhase">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Indicator"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#KillChainPhase"/>
        <rdfs:comment xml:lang="en">The kill chain phase(s) to which this Indicator corresponds.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#label -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#label"/>
    


    <!-- http://ontologies.ti-semantics.com/cti#mitigates -->

