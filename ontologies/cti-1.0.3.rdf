                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Indicator"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#ObservedData"/>
