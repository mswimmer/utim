        <rdfs:subClassOf rdf:resource="http://ontologies.ti-semantics.com/cti#Vocabularies"/>
        <rdfs:comment xml:lang="en">Tool labels describe the categories of tools that can be used to perform attacks.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://docs.oasis-open.org/cti/stix/v2.0/cs01/part1-stix-core/stix-v2.0-cs01-part1-stix-core.html#_Toc496709307</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">tool</rdfs:label>
    </owl:Class>
    


