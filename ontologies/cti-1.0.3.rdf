    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#resourceLevel">
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#AttackResourceLevelVocab"/>
        <rdfs:label xml:lang="en">resource_level</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#revokedBy -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#revokedBy">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">It&apos;s a little unclear what this is supposed to mean. Most likely it&apos;s meant to say that some entity is replaced by another in the description and this one is revoked.</rdfs:comment>
        <rdfs:label>revoked-by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#secondaryMotivation -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#secondaryMotivation">
        <rdfs:subPropertyOf rdf:resource="http://ontologies.ti-semantics.com/cti#motivation"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#sightedAt -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#sightedAt">
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#Sighting"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Identity"/>
        <rdfs:comment xml:lang="en">A list of ID references to the Identity (victim) objects of the entities that saw the sighting.

Omitting the where_sighted_refs property does not imply that the sighting was seen by the object creator. To indicate that the sighting was seen by the object creator, an Identity representing the object creator should be listed in where_sighted_refs.

This property MUST reference only Identity SDOs.</rdfs:comment>
        <rdfs:comment xml:lang="en">where_sighted_refs</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#sightingOf -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#sightingOf">
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#Sighting"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Sightable"/>
        <rdfs:comment xml:lang="en">Reference to something that was sighted (e.g., Indicator or Malware).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#tactic -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#tactic">
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#Matrix"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/cti#Technique"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Tactic"/>
        <mitreVerb rdf:datatype="http://www.w3.org/2001/XMLSchema#token">tactic_refs</mitreVerb>
        <rdfs:comment xml:lang="en">Related a Matric to a relevant tactic covered in the Matrix representation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#targetsExploit -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#targetsExploit">
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/core#Vulnerability"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#targetsVictim -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#targetsVictim">
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Identity"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#threatActorAttributedTo -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#threatActorAttributedTo">
        <rdfs:subPropertyOf rdf:resource="http://ontologies.ti-semantics.com/cti#attributedTo"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://ontologies.ti-semantics.com/core#ThreatActor"/>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/cti#Identity"/>
        <rdfs:comment xml:lang="en">This Relationship describes that the Threat Actor&apos;s real identity is the related Identity.

For example, an attributed-to Relationship from the jay-sm17h Threat Actor to the John Smith Identity means that the actor known as jay-sm17h is John Smith.

Note that &apos;Identity&apos; is a disputed entity in this ontology.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#usedBy -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#usedBy">
        <owl:inverseOf rdf:resource="http://ontologies.ti-semantics.com/cti#uses"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Tool"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://ontologies.ti-semantics.com/core#ThreatActor"/>
    </owl:ObjectProperty>
    


    <!-- http://ontologies.ti-semantics.com/cti#uses -->

    <owl:ObjectProperty rdf:about="http://ontologies.ti-semantics.com/cti#uses">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#ThreatActor"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#IntrusionSet"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/core#Malware"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#AttackPattern"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Technique"/>
                    <rdf:Description rdf:about="http://ontologies.ti-semantics.com/cti#Tool"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Refers to a technique, tool, attack pattern or malware that we have seen a threat actor or intrusion set use.</rdfs:comment>
        <rdfs:label xml:lang="en">uses</rdfs:label>
    </owl:ObjectProperty>
    


