        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines how the initial information for this entry was obtained.</rdfs:comment>
        <rdfs:label>Submission Source</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ontologies.ti-semantics.com/weakness#taxonomyName -->

    <owl:DatatypeProperty rdf:about="http://ontologies.ti-semantics.com/weakness#taxonomyName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the taxonomy to which this weakness has a similar or equivalent entry.</rdfs:comment>
        <rdfs:label>Taxonomy Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ontologies.ti-semantics.com/weakness#techName -->

    <owl:DatatypeProperty rdf:about="http://ontologies.ti-semantics.com/weakness#techName">
