        <rdfs:label>Status</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://ontologies.ti-semantics.com/weakness#structureType -->

    <owl:DatatypeProperty rdf:about="http://ontologies.ti-semantics.com/weakness#structureType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies how this view is being constructed. 

Valid values are: Implicit Slice = a slice based on a filter criteria; Explicit Slice = a slice based on arbitrary membership, as defined by specific relationships between entries; Graph = a bounded graphical slice based on ChildOf relationships</rdfs:comment>
        <rdfs:label>Structure Type</rdfs:label>
    </owl:DatatypeProperty>
    


